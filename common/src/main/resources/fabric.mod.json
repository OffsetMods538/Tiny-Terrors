{
	"schemaVersion": 1,
	"id": "tiny-terrors-common",
	"version": "${modVersion}",
	"name": "Tiny Terrors Common",
	"description": "Adds baby monsters",
	"authors": [
		"OffsetMonkey538"
	],
	"contact": {
		"sources": "https://github.com/OffsetMods538/Tiny-Terrors",
		"issues": "https://github.com/OffsetMods538/Tiny-Terrors/issues",
		"homepage": "https://modrinth.com/mod/tiny-terrors"
	},
	"license": "MIT",
	"icon": "assets/tiny-terrors/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "top.offsetmonkey538.tinyterrors.common.TinyTerrors"
		],
		"client": [
          "top.offsetmonkey538.tinyterrors.common.TinyTerrorsClient"
		]
	},
	"mixins": [
      "tiny-terrors.common.mixins.json",
		{
			"config": "tiny-terrors.common.client.mixins.json",
			"environment": "client"
		}
	],
	"accessWidener": "tiny-terrors.accesswidener",
	"depends": {
		"fabric-api": "*",
		"minecraft": "${supportedMinecraftVersions}",
		"java": ">=21"
	}
}
